<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:xct="clr-namespace:Xamarin.CommunityToolkit.UI.Views;assembly=Xamarin.CommunityToolkit"
             x:Class="AllNotes.NewFolderPage">

    <StackLayout>
        <Button BackgroundColor="Transparent" FontSize="24" Text="Rename" TextColor="Black" WidthRequest="200" Margin="5" Clicked="Button_Clicked"/>
        <Button BackgroundColor="Transparent" FontSize="24" Text="Delete" TextColor="Black" WidthRequest="200" Margin="5" Clicked="Button_Clicked_1"/>
        <Button BackgroundColor="Transparent" FontSize="24" Text="Cancel" TextColor="Blue" WidthRequest="200" Margin="5" Clicked="Button_Clicked_2"/>
    </StackLayout>


</ContentPage>